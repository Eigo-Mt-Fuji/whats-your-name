{
  "name": "whats-your-name",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://eigo-mt-fuji.github.io/whats-your-name",
  "dependencies": {
    "ajv": "^6.0.0",
    "create-react-class": "^15.6.3",
    "jsdom": "^11.6.2",
    "material-ui": "^0.20.0",
    "npm": "^5.8.0",
    "prop-types": "^15.6.1",
    "react": "^16.3.0",
    "react-css-grid": "^2.0.0-0",
    "react-scripts-babel-7": "2.0.5",
    "react-sequence-diagram": "0.0.11",
    "whatwg-fetch": "^2.0.4"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/",
    "build": "npm run build-css && react-scripts build",
    "watch-css": "node-sass-chokidar src/ -o src/ --watch --recursive",
