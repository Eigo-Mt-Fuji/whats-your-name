{
  "name": "whats-your-name",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://eigo-mt-fuji.github.io/whats-your-name",
  "dependencies": {
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-scripts": "1.1.1"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/",
    "build": "npm run build-css && react-scripts build",
    "watch-css": "node-sass-chokidar src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p build-css watch-css start-js",
    "deploy": "npm run build && gh-pages -d build",
    "test": "NODE_ENV=test react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.40",
    "babel-core": "^6.26.0",
    "babel-jest": "^22.4.1",
    "gh-pages": "^1.1.0",
    "jest": "^20.0.4",
    "jest-cli": "^22.4.2",
    "node-sass-chokidar": "^1.1.0",
    "npm-run-all": "^4.1.2",
    "regenerator-runtime": "^0.11.1"
  },
  "presets": [
    "env",
    "react"
  ],
  "env": {
    "test": {
      "presets": [
        [
          "env"
        ],
        "react"
      ]
    }
  }
}
