{
  "name": "whats-your-name",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://eigo-mt-fuji.github.io/whats-your-name",
  "dependencies": {
    "create-react-class": "^15.6.3",
    "material-ui": "^0.20.0",
    "prop-types": "^15.6.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-scripts": "1.1.1",
    "react-sequence-diagram": "0.0.11",
    "whatwg-fetch": "^2.0.3"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/",
    "build": "npm run build-css && react-scripts build",
    "watch-css": "node-sass-chokidar src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p build-css watch-css start-js",
    "deploy": "npm run build && gh-pages -d build",
    "test": "CI=true NODE_ENV=test react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "babel-core": "6.0.0",
    "babel-jest": "^22.4.1",
    "gh-pages": "^1.1.0",
    "jest": "^20.0.4",
    "minimatch": "3.0.2",
    "node-sass-chokidar": "^1.1.0",
    "npm-run-all": "^4.1.2",
    "regenerator-runtime": "^0.11.1"
  }
}
